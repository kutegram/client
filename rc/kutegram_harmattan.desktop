[Desktop Entry]
Encoding=UTF-8
Version=1.0
Type=Application
Name=Kutegram
Comment=An unofficial Qt-based crossplatform client for Telegram messenger.
Exec=/usr/bin/invoker --single-instance --splash=/usr/share/kutegram/splash.png --type=d /opt/usr/bin/kutegram
Icon=/usr/share/icons/hicolor/80x80/apps/kutegram.png
X-Window-Icon=
X-HildonDesk-ShowInToolbar=true
X-Osso-Type=application/x-executable
Categories=Network;Chat;InstantMessaging;Qt;
Terminal=false
